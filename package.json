{
  "name": "nestjs-annotate",
  "version": "1.0.0",
  "description": "NestJS service to add sticky note annotations to uploaded PDF files",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "tsc -p tsconfig.json",
    "generate": "nest build"
  },
  "dependencies": {
    "@nestjs/common": "*",
    "@nestjs/core": "*",
    "@nestjs/platform-express": "*",
    "annotpdf": "^1.0.11",
    "multer": "*"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^5.0.0"
  }
}
